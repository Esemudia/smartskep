{
  "functions": {
    "api/index.php": {
      "runtime": "now-php@1.0.0"
    },
    "api/node/index.js": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    {
      "src": "/php/(.*)",
      "dest": "/api/index.php"
    },
    {
      "src": "/node/(.*)",
      "dest": "/api/node/index.js"
    }
  ]
}
